<%= form_for @recipe do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br><br>

<%= f.fields_for :ingredients do |ing| %>
    <%= ing.label :name %>
    <%= ing.text_field :name %> 
 
    <%= ing.label :quantity %>
    <%= ing.text_field :quantity %><br>
 <br><br>

  <% end %>
  <%= f.submit %>
<% end %>
